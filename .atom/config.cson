"*":
  welcome:
    showOnStartup: false
  core:
    disabledPackages: [
      "exception-reporting"
      "spell-check"
      "welcome"
    ]
    themes: [
      "one-light-ui"
      "one-light-syntax"
    ]
    audioBeep: false
    projectHome: "/home/chris/git"
    ignoredNames: [
      ".git"
      ".idea"
    ]
  editor:
    invisibles: {}
    backUpBeforeSaving: true
    scrollPastEnd: true
    zoomFontWhenCtrlScrolling: false
    fontFamily: "\"DejaVu Sans Mono\""
    fontSize: 12
  autosave:
    enabled: true
  "local-history": {}
  tabs:
    enableVcsColoring: true
  "tree-view":
    hideIgnoredNames: true
  "autocomplete-plus":
    enableAutoActivation: false
    enableAutoConfirmSingleSuggestion: false
