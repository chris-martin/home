{
    "terminal.integrated.shell.linux": "/run/current-system/sw/bin/fish",
    "window.zoomLevel": 0,
    "telemetry.enableTelemetry": false,
    "telemetry.enableCrashReporter": false,
    "editor.fontSize": 10,
    "files.autoSave": "onWindowChange",
    "editor.tabSize": 2,
    "editor.wordWrap": "on",
    "files.exclude": {
      "**/.history": true,
      "**/.stack": true,
      "**/.stack-work": true,
      "**/dist": true,
      "**/dist-newstyle": true
    },
    "editor.mouseWheelZoom": true,
    "editor.minimap.enabled": false,
    "editor.quickSuggestions": false,
    "editor.wordBasedSuggestions": false,
    "workbench.colorTheme": "Visual Studio Light",
    "breadcrumbs.enabled": true,
    "git.autofetch": true,
    "workbench.tree.indent": 28,
    "explorer.confirmDragAndDrop": false,
    "files.trimTrailingWhitespace": true,
    "editor.wrappingIndent": "deepIndent",
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.autoIndent": "keep",
    "actionButtons": {
        "reloadButton": null,
        "commands": [
            {
                "name": "make",
                "singleInstance": true,
                "command": "make"
            },
            {
                "name": "hs-nix-modules",
                "singleInstance": true,
                "command": "make hs-nix-modules"
            },
            {
                "name": "make stylish-haskell",
                "singleInstance": true,
                "command": "make stylish-haskell"
            },
            {
                "name": "make screen",
                "singleInstance": true,
                "command": "make screen"
            },
            {
                "name": "cabal haddock",
                "singleInstance": true,
                "command": "cabal haddock"
            }
        ]
    },
    "todohighlight.keywordsPattern": "\\bTODO\\b|\\b[Tt][Oo][Dd][Oo](?= *[:\\-\\r\\n])|\\[ *[Tt][Oo][Dd][Oo] *\\]|\\( *[Tt][Oo][Dd][Oo] *\\)",
    "editor.dragAndDrop": false,
    "update.enableWindowsBackgroundUpdates": false,
    "update.mode": "none",
    "update.showReleaseNotes": false,
    "editor.detectIndentation": false,
    "vs-color-picker.autoLaunch": false,
    "editor.parameterHints.enabled": false
}