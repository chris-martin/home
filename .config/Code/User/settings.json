{
    "todohighlight.include": [
        "**/*.md",
        "**/*.txt",
        "**/*.hs",
        "**/*.hsc",
        "**/*.hs.boot",
        "**/*.hsc.boot"
    ],
    "terminal.integrated.shell.linux": "/run/current-system/sw/bin/fish",
    "telemetry.enableTelemetry": false,
    "telemetry.enableCrashReporter": false,
    "terminal.integrated.confirmOnExit": "hasChildProcesses",
    "terminal.integrated.confirmOnKill": "always",
    "files.autoSave": "onWindowChange",
    "editor.tabSize": 2,
    "editor.wordWrap": "on",
    "files.exclude": {
      "**/.history": true,
      "**/.stack": true,
      "**/.stack-work": true,
      "**/*.dyn_hi": true,
      "**/*.dyn_o": true,
      "**/*.hi": true,
      "**/*.o": true,
      "**/dist": true,
      "**/dist-newstyle": true
    },
    "editor.quickSuggestions": {
      "comments": "off",
      "strings": "off",
      "other": "off"
    },
    "editor.wordBasedSuggestions": false,
    "workbench.colorTheme": "Visual Studio Light",
    "breadcrumbs.enabled": false,
    "git.autofetch": true,
    "workbench.tree.indent": 28,
    "explorer.confirmDragAndDrop": false,
    "files.trimTrailingWhitespace": true,
    "editor.wrappingIndent": "deepIndent",
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.autoIndent": "keep",
    "editor.renderWhitespace": "none",
    "editor.renderControlCharacters": false,
    "actionButtons": {
        "reloadButton": null,
        "commands": [
            {
                "name": "make",
                "singleInstance": true,
                "command": "make"
            },
            {
                "name": "hs-nix-modules",
                "singleInstance": true,
                "command": "make hs-nix-modules"
            },
            {
                "name": "make stylish-haskell",
                "singleInstance": true,
                "command": "make stylish-haskell"
            },
            {
                "name": "make screen",
                "singleInstance": true,
                "command": "make screen"
            },
            {
                "name": "cabal haddock",
                "singleInstance": true,
                "command": "cabal haddock"
            },
            {
                "name": "stack haddock",
                "singleInstance": true,
                "command": "stack haddock"
            }
        ]
    },
    "todohighlight.keywordsPattern": "\\bTODO\\b|\\b[Tt][Oo][Dd][Oo](?= *[:\\-\\r\\n])|\\[ *[Tt][Oo][Dd][Oo] *\\]|\\( *[Tt][Oo][Dd][Oo] *\\)|\\b_[Tt][Oo][Dd][Oo]\\b",
    "editor.dragAndDrop": false,
    "update.enableWindowsBackgroundUpdates": false,
    "update.mode": "none",
    "update.showReleaseNotes": false,
    "editor.detectIndentation": false,
    "vs-color-picker.autoLaunch": false,
    "editor.parameterHints.enabled": false,
    "editor.autoClosingQuotes": "never",
    "editor.autoClosingBrackets": "never",
    "explorer.confirmDelete": false,
    "explorer.enableDragAndDrop": false,
    "workbench.panel.defaultLocation": "right",
    "editor.autoClosingOvertype": "never",
    "editor.find.seedSearchStringFromSelection": false,
    "terminal.integrated.scrollback": 10000,
    "explorer.autoReveal": "focusNoScroll",
    "diffEditor.renderSideBySide": false,
    "workbench.sash.size": 12,
    "[haskell]": {
      "editor.defaultFormatter": "haskell.haskell"
    },
    "haskell.formattingProvider": "stylish-haskell",
    "editor.autoSurround": "never",
    "haskell.openDocumentationInHackage": false,
    "haskell.openSourceInHackage": false,
    "files.associations": {
      "*.pro": "prosidy"
    },
    "debug.console.fontSize": 12,
    "window.title": "${rootName}${separator}${dirty}",
    "window.titleSeparator": "  ",
    "haskell.plugin.hlint.diagnosticsOn": false,
    "cSpell.userWords": [
