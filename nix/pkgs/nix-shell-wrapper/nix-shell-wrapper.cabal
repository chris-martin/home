name:                nix-shell-wrapper
version:             0.1.0.0
license:             Apache-2.0
build-type:          Simple
cabal-version:       >=1.10

executable haskell
  hs-source-dirs:      .
  main-is:             Haskell.hs
  ghc-options:         -fwarn-unused-imports
  build-depends:       base >= 4.7 && < 5
                     , directory
                     , path
                     , posix-escape
                     , process
  default-language:    Haskell2010
